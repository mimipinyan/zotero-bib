@article{liechtyResponseMidrotationPine2013,
  title = {Response of {{Midrotation Pine Stands}} to {{Fertilizer}} and {{Herbicide Application}} in the {{Western Gulf Coastal Plain}}},
  author = {Liechty, Hal O. and Fristoe, Conner},
